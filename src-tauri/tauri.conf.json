{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "4Seasons",
  "version": "0.1.0",
  "identifier": "com.seasonality.app",
  "build": {
    "frontendDist": "dist",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "4Seasons - Commodity Seasonality",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "../dist/seasonality"
    ],
    "externalBin": [
      "binaries/seasonality",
      "binaries/cot_data"
    ]
  },
  "plugins": {}
}
